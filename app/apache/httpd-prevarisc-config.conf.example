# Generic parameters
SetEnv PREVARISC_DEBUG_ENABLED 1
SetEnv PREVARISC_SECURITY_SALT ""
SetEnv PREVARISC_ENFORCE_SECURITY 0
SetEnv PREVARISC_SESSION_MAX_LIFETIME 3600

# Database configuration
SetEnv PREVARISC_DB_ADAPTER "mysqli"
SetEnv PREVARISC_DB_HOST "db"
SetEnv PREVARISC_DB_CHARSET "utf8"
SetEnv PREVARISC_DB_USERNAME ""
SetEnv PREVARISC_DB_PASSWORD ""
SetEnv PREVARISC_DB_DBNAME "PRV_prevarisc_v2"
SetEnv PREVARISC_DB_PORT 33061

# Cache configuration
SetEnv PREVARISC_CACHE_LIFETIME "3600"
SetEnv PREVARISC_CACHE_ADAPTER "File"
SetEnv PREVARISC_CACHE_DIR "/home/prv/cache"

# LDAP Configuration
SetEnv PREVARISC_LDAP_ENABLED 0
SetEnv PREVARISC_LDAP_HOST ""
SetEnv PREVARISC_LDAP_PORT "389"
SetEnv PREVARISC_LDAP_USERNAME ""
SetEnv PREVARISC_LDAP_PASSWORD ""
SetEnv PREVARISC_LDAP_BASEDN ""

# Clé d'authentification API
SetEnv PREVARISC_SECURITY_KEY ""

SetEnv PREVARISC_MAIL_ENABLED 0

# Couches cartographiques
SetEnv PREVARISC_PLUGIN_IGNKEY "essentiels,adresse,administratif,cartes,cartovecto"
SetEnv PREVARISC_IGN_AUTOCONF "/ign/prevarisc/customConfig.json"
