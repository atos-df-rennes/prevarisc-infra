# CAS
SetEnv PREVARISC_CAS_ENABLED 0
SetEnv PREVARISC_CAS_HOST "rns090lv.rennes.si.fr.atosorigin.com"
SetEnv PREVARISC_CAS_PORT "8080"
SetEnv PREVARISC_CAS_CONTEXT "cas"
SetEnv PREVARISC_CAS_NO_SERVER_VALIDATION "1"
SetEnv PREVARISC_CAS_VERSION "2.0"

# SSO NTLM
SetEnv PREVARISC_NTLM_ENABLED 0

# Parametres specifiques
SetEnv PREVARISC_LOCAL_SOMMEIL_TYPES "1;5"
SetEnv PREVARISC_COMMISSION_COMMUNALE_TYPE ""
SetEnv PREVARISC_RISQUES_NATURELS_EXEMPLES "Avalanche, Cyclonique, Feux de Forêt, Inondation, Mouvement de terrain, Sismique, Submersion marine, Tempête, Volcanique"
SetEnv PREVARISC_RISQUES_TECHNOLOGIQUES_EXEMPLES "Industriel, Nucléaire, Rupture de barrage, Transport de matières dangereuses"
SetEnv PREVARISC_DASHBOARD_NEXT_COMMISSIONS_DAYS 15
SetEnv PREVARISC_DASHBOARD_DOSSIERS_SANS_AVIS_DAYS 15
SetEnv PREVARISC_DASHBOARD_COURRIER_SANS_REPONSE_DAYS 10

# Visualiseur géobretagne
# SetEnv PREVARISC_PLUGIN_GEOBRETAGNE "https://geobretagne.fr/m/"
# SetEnv PREVARISC_PLUGIN_GEOBRETAGNE "http://localhost:7080/js/sdis29/sviewer/index.html"

# Proxy
SetEnv PREVARISC_PROXY_ENABLED 0
SetEnv PREVARISC_PROXY_PROTOCOL "HTTP"
SetEnv PREVARISC_PROXY_PORT "8080"
SetEnv PREVARISC_PROXY_HOST "127.0.0.1"
SetEnv PREVARISC_PROXY_USERNAME ""

# Set data path where all the data is stored
SetEnv PREVARISC_REAL_DATA_PATH "/mnt/prevarisc-data"
Define PREVARISC_REAL_DATA_PATH "/mnt/prevarisc-data"
SetEnv PREVARISC_DATASTORE_ADAPTER "Plugin_HashedFileDataStore"
# SetEnv PREVARISC_DATASTORE_ADAPTER "Plugin_SimpleFileDataStore"

# Utiliser la date de commission pour relancer la périodicité
SetEnv PREVARISC_DATE_COMMISSION_RELANCE_PERIODICITE 0

# Utiliser le nouveau descriptif
SetEnv PREVARISC_DESCRIPTIF_PERSONNALISE 1

# Passer la périodicité en années
SetEnv PREVARISC_UNITE_PERIODICITE_ANNEES 0

# Désactive tous les affichages liés à Plat'AU
SetEnv PREVARISC_DEACTIVATE_PLATAU 0

# Nombre maximum de dossiers à afficher en simultané
# SetEnv PREVARISC_DOSSIERS_MAX_A_AFFICHER 8

# Utiliser l'API Adresse plutôt que la base locale Prevarisc
SetEnv PREVARISC_API_ADRESSE 0

# Utiliser l'affichage Bootstrap v3 plutôt que Boostrap v2
SetEnv PREVARISC_BOOTSTRAP_3 0